{
    "name": "auth-extension",
    "version": "0.3.3",
    "private": true,
    "description": "Ente Auth browser extension for secure 2FA autofill",
    "scripts": {
        "build": "npm run build:chrome && npm run build:firefox",
        "build:chrome": "cross-env BROWSER=chrome webpack --config webpack.config.js",
        "build:firefox": "cross-env BROWSER=firefox webpack --config webpack.config.js",
        "dev": "webpack --config webpack.config.js --watch",
        "dev:chrome": "cross-env BROWSER=chrome webpack --config webpack.config.js --watch",
        "dev:firefox": "cross-env BROWSER=firefox webpack --config webpack.config.js --watch"
    },
    "dependencies": {
        "assert": "^2.1.0",
        "buffer": "^6.0.3",
        "crypto-browserify": "^3.12.1",
        "fast-srp-hap": "^2.0.4",
        "jsqr": "^1.4.0",
        "jssha": "~3.3.1",
        "libsodium-wrappers-sumo": "^0.7.15",
        "otpauth": "9.2.4",
        "process": "^0.11.10",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "stream-browserify": "^3.0.0",
        "vm-browserify": "^1.1.2",
        "webextension-polyfill": "^0.12.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.287",
        "@types/react": "^19.1.0",
        "@types/react-dom": "^19.1.0",
        "@types/webextension-polyfill": "^0.12.1",
        "copy-webpack-plugin": "^12.0.2",
        "cross-env": "^10.1.0",
        "css-loader": "^7.1.2",
        "html-webpack-plugin": "^5.6.3",
        "mini-css-extract-plugin": "^2.9.2",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.8.3",
        "webpack": "^5.97.1",
        "webpack-cli": "^6.0.1"
    }
}
